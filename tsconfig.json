{
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": "./",
        "jsx": "react",
        "lib": ["dom", "es2015"],
        "module": "esNext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./dist/",
        "paths": {
            "config": ["./config.json"],
            "types/*": ["./src/types/*"],
            "utils/*": ["./src/utils/*"]
        },
        "sourceMap": true,
        "target": "es5"
    }
}
